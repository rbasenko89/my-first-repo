{
  "consumer": {
    "name": "qa-consumer-petstore"
  },
  "interactions": [
    {
      "description": "Create a new pet",
      "providerStates": [
        {
          "name": "Pet can be created"
        }
      ],
      "request": {
        "body": {
          "id": 123456,
          "name": "Fluffy",
          "status": "available"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "method": "POST",
        "path": "/v2/pet"
      },
      "response": {
        "body": {
          "id": 123456,
          "name": "Fluffy",
          "status": "available"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "Get pet by id",
      "providerStates": [
        {
          "name": "Pet exists with id 123456"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/v2/pet/123456"
      },
      "response": {
        "body": {
          "id": 123456,
          "name": "Fluffy",
          "status": "available"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "Get pet by unknown id",
      "providerStates": [
        {
          "name": "Pet does not exist with id 99999999"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/v2/pet/99999999"
      },
      "response": {
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 404
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "16.0.2"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "petstore"
  }
}